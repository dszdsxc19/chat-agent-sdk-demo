{
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "dev:react-playground": "turbo run dev --filter=react-playground",
    "dev:solvte-playground": "turbo run dev --filter=solvte-playground",
    "build:sdk": "turbo run build --filter=my-agent-sdk",
    "build:web-component": "turbo run build --filter=agent-widget",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "turbo": "^2.8.0"
  },
  "packageManager": "pnpm@8.15.6",
  "name": "with-vite",
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
